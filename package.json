		"supabase:studio": "pnpm dlx supabase status --output json | jq -r '.[\"Studio URL\"]' | xargs start",
		"supabase:reset": "pnpm dlx supabase db reset",
		"supabase:gen-types": "pnpm dlx supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public > src/lib/supabase/types.ts"
	},
