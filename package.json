	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"start": "next start",
		"supabase:start": "pnpm dlx supabase start",
		"supabase:stop": "pnpm dlx supabase stop",
